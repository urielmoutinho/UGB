@model IEnumerable<UgbFront.Models.ServicoInternoModel>
<p>
    <a asp-action="Create" class="btn btn-primary">Novo Serviço Interno</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Data de Cadastro</th>
            <th>Observação</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var servicoInterno in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => servicoInterno.ServicoInternoId)</td>
                <td>@Html.DisplayFor(modelItem => servicoInterno.DataCadastro)</td>
                <td>@Html.DisplayFor(modelItem => servicoInterno.Descricao)</td>
                <td>
                    <a asp-action="Details" asp-route-id="@servicoInterno.ServicoInternoId" class="btn btn-info">Detalhes</a>
                    <a asp-action="Edit" asp-route-id="@servicoInterno.ServicoInternoId" class="btn btn-warning">Editar</a>
                    <a asp-action="Delete" asp-route-id="@servicoInterno.ServicoInternoId" class="btn btn-danger">Excluir</a>
                </td>
            </tr>
        }
    </tbody>
</table>