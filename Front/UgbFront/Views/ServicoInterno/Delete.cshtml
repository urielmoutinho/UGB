@model UgbFront.Models.ServicoInternoModel

<h1>Excluir Servico Interno</h1>



<div>
    <h4>ID do Serviço Interno: @Model.ServicoInternoId</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Data de Cadastro:</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.DataCadastro)</dd>
        <dt class="col-sm-2">Observação:</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Descricao)</dd>
    </dl>
</div>

<form method="post" action="@Url.Action("Delete", "ServicoInterno", new { id = Model.ServicoInternoId })">
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>